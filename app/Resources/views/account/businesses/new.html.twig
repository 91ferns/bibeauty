{% extends 'admin.html.twig' %}

{% block body %}

  {% include 'shared/adminheader.html.twig' %}

<div class="panel panel-default">
  <div class="panel-body">
    <div class="row">
      <div class="col-md-7">


        {{ form_start(form, {'attr': { 'novalidate': 'true' }} ) }}
        <div class="row">
          <div class="col-md-4">
            {{ form_row(form.headerAttachment) }}
          </div>
          <div class="col-md-4">
            {{ form_row(form.logoAttachment) }}
          </div>
        </div>
        {{ form_widget(form) }}
          <input type="submit" value="Create"
               class="btn btn-default pull-right" />
        {{ form_end(form) }}


      </div> <!-- /.col-md-7 -->
    </div>
  </div>
</div>


{% endblock %}
