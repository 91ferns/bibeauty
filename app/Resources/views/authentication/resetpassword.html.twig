{% extends 'base.html.twig' %}

{% block body %}

<section id="resetpassword" class="nav-pad">

  <div class="container">
    <div class="col-lg-offset-3 col-lg-6 col-lg-offset-3 col-md-offset-3 col-md-6 col-md-offset-3 col-sm-offset-2 col-sm-8 col-sm-offset-2 col-xs-offset-1 col-xs-10 col-xs-offset-1">

        {% if error %}
          <div class="alert alert-warning">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
        {% endif %}
         <h1 class="login-title"><strong>Please enter your new password. </strong></h1>
       {{ form(form) }}
      </div>
  </div>

</section>

{% endblock %}
